  <xsl:template name="pnlMain2">
<xsl:param name="ui-page" select="/root/page"/>
    <xsl:param name="base"/>
      
    page3.xsl: pnlMain2<br/>
    
    
<xsl:if test="$ui-page/btnOK/visible = '1'">
<input id="page-btnOK" name="page-btnOK" style="width: 200px" value="Button3" type="button">
<xsl:if test="count( $ui-page/btnOK/onclick/handlers/* ) != 0 and $ui-page/btnOK/autoPostBack = '1'"><xsl:attribute name="onclick">_temis.sendMessage(this,_temis.createEvent('onclick','page-btnOK'),_temis.createTarget('',''));</xsl:attribute></xsl:if></input></xsl:if>
    
  </xsl:template><xsl:variable name="global-url">
    url
  </xsl:variable><xsl:template name="pnlMain1">
<xsl:param name="ui-page" select="/root/page"/>

      page2.xsl: pnlMain1<br/>
      
    
