<input id="page-btnOK" name="page-btnOK" style="width: 200px" type="button"><xsl:choose><xsl:when test="$ui-page/btnOK/text != ''"><xsl:attribute name="value"><xsl:value-of select="$ui-page/btnOK/text"/></xsl:attribute></xsl:when></xsl:choose>
