            <!-- ///////////////////////////////////////////////////////////////////////////////////////// -->
            <tr style="background-color: #90ECEC;">
              <th colspan="3">using xsl:include, xsl:for-each, JavaScript handling </th>
            </tr>
            
            <tr>
              <td>
                xsl:include
              </td>
              <td>
                <xsl:call-template name="pnlMain1">
<xsl:with-param name="ui-page" select="$ui-page"/></xsl:call-template><br/>
                <xsl:call-template name="pnlMain2">
<xsl:with-param name="ui-page" select="$ui-page"/></xsl:call-template>
              </td>
              <td>processing for &lt;xsl:include&gt;<br/>
              <code><pre style="margin:0;">
              
&lt;xsl:call-template name="pnlMain1"/&gt;&lt;br/&gt;
&lt;xsl:call-template name="pnlMain2"/&gt;
                
              </pre></code>
              
            </td>
            </tr>
            <tr>
              <td>xsl:for-each</td>
              <td>
                <xsl:for-each select="//page/data/*">
                  <xsl:variable name="item" select="current()"/>
                  
<xsl:if test="$ui-page/rbRadioButton/visible = '1'">
<input id="page-rbRadioButton_{$item}" name="page-rbRadioButton" type="radio" value="{$item}">
<xsl:if test="count( $ui-page/rbRadioButton/onclick/handlers/* ) != 0 and $ui-page/rbRadioButton/autoPostBack = '1'"><xsl:attribute name="onclick">_temis.sendMessage(this,_temis.createEvent('onclick','page-rbRadioButton'),_temis.createTarget('',''));</xsl:attribute></xsl:if>
<xsl:if test="count( $ui-page/rbRadioButton/onchange/handlers/* ) != 0 and $ui-page/rbRadioButton/autoPostBack = '1'"><xsl:attribute name="onchange">_temis.sendMessage(this,_temis.createEvent('onchange','page-rbRadioButton'),_temis.createTarget('',''));</xsl:attribute></xsl:if>
<xsl:variable name="value" select="$item"/>
<xsl:if test="$ui-page/rbRadioButton/value = $value"><xsl:attribute name="checked">1</xsl:attribute></xsl:if>
<xsl:attribute name="value"><xsl:value-of select="$value"/></xsl:attribute></input></xsl:if>
                  
<xsl:if test="$ui-page/cbCheckBox/visible = '1'">
<input id="page-cbCheckBox${@index}" name="page-cbCheckBox[{@index}]" type="checkbox">
<xsl:if test="count( $ui-page/cbCheckBox/onclick/handlers/* ) != 0 and $ui-page/cbCheckBox/autoPostBack = '1'"><xsl:attribute name="onclick">_temis.sendMessage(this,_temis.createEvent('onclick','page-cbCheckBox','{@index}'),_temis.createTarget('',''));</xsl:attribute></xsl:if>
<xsl:if test="count( $ui-page/cbCheckBox/onchange/handlers/* ) != 0 and $ui-page/cbCheckBox/autoPostBack = '1'"><xsl:attribute name="onchange">_temis.sendMessage(this,_temis.createEvent('onchange','page-cbCheckBox','{@index}'),_temis.createTarget('',''));</xsl:attribute></xsl:if>
<xsl:variable name="value">
<xsl:value-of select="$ui-page/cbCheckBox/checked/*[ @index = current()/@index]"/></xsl:variable>
<xsl:if test="$value = '1'"><xsl:attribute name="checked">1</xsl:attribute></xsl:if></input></xsl:if>
                </xsl:for-each>
              </td>
              <td>
                <code><pre style="margin:0;">
&lt;xsl:for-each select="//page/data/*"&gt;
  &lt;xsl:variable name="item" select="current()"/&gt;
  &lt;ui:radio id="rbRadioButton" value="{$item}"/&gt;
  &lt;ui:checkbox id="cbCheckBox" ui:index="{@index}"/&gt;
&lt;/xsl:for-each&gt;
</pre></code>
              </td>
            </tr>
            <tr>
              <td>OnSubmit() handing</td>
              <td>
                <input type="button" value="set handler" onclick="setHandler();"/>
                <script>
function setHandler( event )
{
   _temis.onSubmit.addAction( "alert( 'user action' );" );
}
                </script>
             </td>
             <td>
                <code><pre style="margin:0;">
&lt;script&gt;
&lt;input type="button"  value="set handler" onclick="setHandler();" );" /&gt;
function setHandler( event )
{
   _temis.onSubmit.addAction( "alert( 'user action onSubmit()' );" );
}
&lt;/script&gt;

</pre></code>

             </td>
            </tr>
          </table>

          <p>
            <b>Note:</b>You can override some attributes from XSL template.
          </p>

